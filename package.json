{
  "name": "ai-commission-management-system",
  "version": "1.0.0",
  "description": "AI-First Commission Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Backend build complete'",
    "install:all": "npm install && cd frontend && npm install && cd ../ai-engine && pip install -r requirements.txt"
  },
  "keywords": ["ai", "commission", "management", "reward", "nlp"],
  "author": "AI Commission Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "pdfkit": "^0.14.0",
    "exceljs": "^4.4.0",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 